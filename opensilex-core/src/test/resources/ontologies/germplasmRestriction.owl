<?xml version="1.0"?>
<rdf:RDF
    xmlns="http://www.opensilex.org/vocabulary/test-germplasm-restriction#"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:xml="http://www.w3.org/XML/1998/namespace"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:oeso="http://www.opensilex.org/vocabulary/oeso#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.opensilex.org/vocabulary/test-germplasm-restriction">
        <dc:creator>Valentin Rigolle</dc:creator>
        <rdfs:label xml:lang="en">Test ontology for germplasm restrictions</rdfs:label>
    </owl:Ontology>

    <owl:Class rdf:about="http://www.opensilex.org/vocabulary/oeso#ScientificObject">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.opensilex.org/vocabulary/oeso#hasGermplasm"/>
                <owl:onClass rdf:resource="http://www.opensilex.org/vocabulary/oeso#Germplasm"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

</rdf:RDF>
