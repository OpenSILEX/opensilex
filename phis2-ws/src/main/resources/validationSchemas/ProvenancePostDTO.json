{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "array",
  "title": "The Root Schema",
  "items": {
    "$id": "#/items",
    "type": "object",
    "title": "The Items Schema",
    "required": [
      "label",
      "metadata"
    ],
    "properties": {
      "label": {
        "$id": "#/items/properties/label",
        "type": "string",
        "title": "The Label Schema",
        "default": "",
        "examples": [
          "PROV-SUA3-LEAF"
        ],
        "pattern": "^(.*)$"
      },
      "comment": {
        "$id": "#/items/properties/comment",
        "type": "string",
        "title": "The Comment Schema",
        "default": "",
        "examples": [
          "In this provenance we have count the number of leaf per plant"
        ],
        "pattern": "^(.*)$"
      },
      "metadata": {
        "$id": "#/items/properties/metadata",
        "type": "object",
        "title": "The Metadata Schema",
        "required": [
          "namespaces",
          "creationDate",
          "prov:Agent"
        ],
        "properties": {
          "namespaces": {
            "$id": "#/items/properties/metadata/properties/namespaces",
            "type": "object",
            "title": "The Namespaces Schema",
            "required": [
              "prov",
              "oeso",
              "rdf"
            ],
            "properties": {
              "prov": {
                "$id": "#/items/properties/metadata/properties/namespaces/properties/prov",
                "type": "string",
                "title": "The Prov Schema",
                "default": "",
                "examples": [
                  "http://www.w3.org/ns/prov#"
                ],
                "pattern": "^(.*)$"
              },
              "oeso": {
                "$id": "#/items/properties/metadata/properties/namespaces/properties/oeso",
                "type": "string",
                "title": "The Oeso Schema",
                "default": "",
                "examples": [
                  "http://www.opensilex.org/vocabulary/oeso#"
                ],
                "pattern": "^(.*)$"
              },
              "rdf": {
                "$id": "#/items/properties/metadata/properties/namespaces/properties/rdf",
                "type": "string",
                "title": "The Rdf Schema",
                "default": "",
                "examples": [
                  "http://www.w3.org/1999/02/22-rdf-syntax-ns#"
                ],
                "pattern": "^(.*)$"
              }
            }
          },
          "creationDate": {
            "$id": "#/items/properties/metadata/properties/creationDate",
            "type": "string",
            "title": "The Creationdate Schema",
            "default": "",
            "examples": [
              "2019-11-08T14:36:34+0100"
            ],
            "pattern": "^(.*)$"
          },
          "prov:Agent": {
            "$id": "#/items/properties/metadata/properties/prov:Agent",
            "type": "object",
            "title": "The Prov:agent Schema",
            "required": [
              "oeso:SensingDevice",
              "oeso:Operator"
            ],
            "properties": {
              "oeso:SensingDevice": {
                "$id": "#/items/properties/metadata/properties/prov:Agent/properties/oeso:SensingDevice",
                "type": "array",
                "title": "The Oeso:sensingdevice Schema",
                "items": {
                  "$id": "#/items/properties/metadata/properties/prov:Agent/properties/oeso:SensingDevice/items",
                  "type": "string",
                  "title": "The Items Schema",
                  "default": "",
                  "examples": [
                    "http://www.opensilex.org/sunagri/2018/s18002"
                  ],
                  "pattern": "^(.*)$"
                }
              },
              "oeso:Operator": {
                "$id": "#/items/properties/metadata/properties/prov:Agent/properties/oeso:Operator",
                "type": "array",
                "title": "The Oeso:operator Schema",
                "items": {
                  "$id": "#/items/properties/metadata/properties/prov:Agent/properties/oeso:Operator/items",
                  "type": "string",
                  "title": "The Items Schema",
                  "default": "",
                  "examples": [
                    "http://www.sunagri.fr/sunagri/id/agent/admin_sunagri"
                  ],
                  "pattern": "^(.*)$"
                }
              }
            }
          }
        }
      }
    }
  }
}